{
  "name": "admin-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api-beyou:generate": "openapi-typescript ./src/api/clients/beyou/spec.json -o ./src/api/clients/beyou/api.d.ts",
    "api-beyou:update-local-unix": "curl -s http://localhost:5062/swagger/v1/swagger.json -o ./src/api/clients/beyou/spec.json && sed -i '' 's/; x-api-version=[0-9]*//' ./src/api/clients/beyou/spec.json && npx openapi-typescript ./src/api/clients/beyou/spec.json -o ./src/api/clients/beyou/api.d.ts --bare",
    "api-beyou:update-local-win": "curl -s http://localhost:5062/swagger/v1/swagger.json -o ./src/api/clients/beyou/spec.json && powershell -Command \"(Get-Content ./src/api/clients/beyou/spec.json) -replace '; x-api-version=[0-9]*', '' | Set-Content ./src/api/clients/beyou/spec.json\" && npx openapi-typescript ./src/api/clients/beyou/spec.json -o ./src/api/clients/beyou/api.d.ts --bare"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "lodash": "^4.17.21",
    "next": "15.3.3",
    "openapi-typescript": "^7.8.0",
    "openapi-typescript-fetch": "^2.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.57.0",
    "yup": "^1.6.1",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.17",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/yup": "^0.29.14",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
